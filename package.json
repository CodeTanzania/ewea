{
  "name": "@codetanzania/ewea",
  "version": "0.1.0",
  "description": "Emergency Early Warning, Early Action Toolkit",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ seeds/ scripts/",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --timeout=8000 --require @babel/register test/**/*.spec.js",
    "test:unit": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/unit/**/*.spec.js",
    "test:integration": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/integration/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog",
    "chore:deps": "ncu -u && npm install --save",
    "lint:config": "eslint --print-config rollup.config.js",
    "dev": "npm run seed && npm run start:dev",
    "start:dev": "NODE_ENV=development node server.js",
    "start": "NODE_ENV=production node server.js",
    "seedData": "node scripts/seedData.js",
    "syncIndexes": "node scripts/syncIndexes.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeTanzania/ewea.git"
  },
  "keywords": [
    "codetanzania",
    "emis",
    "ewea",
    "disaster",
    "hazard",
    "incident",
    "emergency",
    "alert",
    "warning",
    "event",
    "risk"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/ewea/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/CodeTanzania/ewea",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@benmaruchu/faker": "4.3.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/travis-cli": "^8.2.0",
    "@lykmapipo/express-test-helpers": "^0.8.8",
    "@lykmapipo/mongoose-test-helpers": "0.7.23",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "doxdox": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.9.8",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.4.2",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "lodash.template": "^4.5.0",
    "mocha": "^6.2.1",
    "npm-check-updates": "^3.1.23",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "@codetanzania/emis-feature": ">=1.4.3",
    "@codetanzania/emis-role": ">=1.5.2",
    "@codetanzania/emis-stakeholder": ">=1.9.0",
    "@lykmapipo/common": ">=0.28.1",
    "@lykmapipo/env": ">=0.14.3",
    "@lykmapipo/express-common": ">=0.17.12",
    "@lykmapipo/express-rest-actions": ">=0.8.0",
    "@lykmapipo/file": ">=0.1.9",
    "@lykmapipo/i18n": ">=0.1.14",
    "@lykmapipo/kue-common": ">=0.3.12",
    "@lykmapipo/logger": ">=0.3.11",
    "@lykmapipo/mongoose-common": ">=0.26.0",
    "@lykmapipo/mongoose-exportable": ">=0.3.11",
    "@lykmapipo/permission": ">=0.8.10",
    "@lykmapipo/postman": ">=0.13.2",
    "@lykmapipo/predefine": ">=1.0.0",
    "async": ">=3.1.0",
    "express": ">=4.17.1",
    "kue": ">=0.11.6",
    "kue-scheduler": ">=0.9.8",
    "lodash": ">=4.17.15",
    "moment": ">=2.24.0",
    "moment-timezone": ">=0.5.26",
    "mongoose": ">=5.7.3",
    "mongoose-geojson-schemas": ">=0.10.12",
    "mongoose-kue": ">=0.8.8",
    "mongoose-rest-actions": ">=0.29.3",
    "redis": ">=2.8.0"
  },
  "engines": {
    "node": ">=12.4.0",
    "npm": ">=6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
